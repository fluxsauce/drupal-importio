/**
 * Implements hook_enable().
 */
function feeds_import_io_enable() {
  cache_clear_all('plugins:feeds:plugins', 'cache');
}

/**
 * Implements hook_disable().
 */
function feeds_import_io_disable() {
  cache_clear_all('plugins:feeds:plugins', 'cache');
}
